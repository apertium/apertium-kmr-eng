            </tags>
            <lu><lit v="in"/><lit-tag v="pr"/></lu>
            <b pos="1"/>
            <lu>
              <clip pos="3" side="tl" part="lem"/><lit-tag v="num"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: di saet num">
      <pattern>
        <pattern-item n="di"/>
        <pattern-item n="saet"/>
        <pattern-item n="num"/>
      </pattern>
      <action>
        <out>
          <chunk name="time" case="caseFirstWord">
