  </rule>

  <rule weight="1.1">
    <match lemma="keç" tags="n.*"><select lemma="daughter" tags="n.*"/></match>
    <match tags="prn.pers.*"/>
  </rule>

  <rule weight="1.1">
    <match lemma="za" tags="n.*"><select lemma="son" tags="n.*"/>
