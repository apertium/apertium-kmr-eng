	  </match>
   	<match lemma="teorîk" tags="adj.*"/>
	</rule>
    
  <rule weight="1.0">
    <match lemma="eql" tags="n.*"><select lemma="mind" tags="n.*"/></match>
  </rule>
    
  <rule weight="1.1">
	  <match lemma="eql" tags="n.*">
	    <select lemma="reason" tags="n.*"/>
	  </match>
   	<match lemma="sereke" tags="adj"/>
	</rule>
    
  <rule weight="1.0">
    <match lemma="nerm" tags="adj.*"><select lemma="gentle" tags="adj.*"/></match>
  </rule>
    
  <rule weight="1.1">
    <or>   	
      <match lemma="gil" tags="n.*"/>
   	  <match lemma="goşt" tags="n.*"/>
    </or>
	  <match lemma="nerm" tags="adj.*">
	    <select lemma="soft" tags="adj.*"/>
	  </match>
	</rule>
</rules>
