  <rule weight="1.0">
    <match lemma="goşt" tags="n.*"><select lemma="flesh" tags="n.*"/></match>
  </rule>

  <rule weight="1.1">
	  <match lemma="goşt" tags="n.*">
	    <select lemma="meat" tags="n.*"/>
	  </match>
   	<or>
	    <match lemma="hûrkirî" tags="adj.*"/>
   	  <match lemma="pûç" tags="adj.*"/>
    </or>
	</rule>

  <rule weight="1.1">
   	<match lemma="goşt" tags="n.*"/>
	  <match lemma="hûrkirî" tags="adj.*">
	    <select lemma="ground" tags="adj.*"/>
	  </match>
	</rule>

  <rule weight="1.0">
    <match lemma="lajwerdî" tags="adj.*"><select lemma="metallic" tags="adj.*"/></match>
  </rule>
    
  <rule weight="1.1">
   	<match lemma="mekteb" tags="n.*"/>
