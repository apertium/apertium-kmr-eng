    <match tags="prn.pers.*"/>
  </rule>

  <rule weight="1.0">
    <match lemma="bi" tags="pr.*"><select lemma="with" tags="pr.*"/></match>
  </rule> 

  <rule weight="1.8">
	  <match lemma="bi" tags="pr.*">
	    <select lemma="in" tags="pr.*"/>
	  </match>
     <or>
      <match lemma="ziman" tags="n.*"/>
      <match lemma="Kurdî"/>
