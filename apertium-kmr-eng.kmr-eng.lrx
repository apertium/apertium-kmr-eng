	</rule>

  <rule weight="1.1">
	  <match lemma="bi" tags="pr.*">
	    <select lemma="with" tags="pr.*"/>
	  </match>
     <or>
	    <match lemma="hiştin"/>
	    <match lemma="şidet"/>
      <match lemma="wêr"/> 
      <match tags="prn.pers.*"/>
     </or>
	</rule>

  <rule weight="1.9">
	  <match lemma="di" tags="pr.*">
	    <select lemma="in" tags="pr.*"/>
	  </match>
   	<match tags="np.*"/>
	</rule>		
    
  <rule weight="1.0">
      <match lemma="li ser" tags="pr.*"><select lemma="above" tags="pr.*"/></match>
  </rule>
        
  <rule weight="1.1">
	  <match lemma="li ser" tags="pr.*">
	    <select lemma="on" tags="pr.*"/>
	  </match>
   	<or>
      <match lemma="pist" tags="n.*"/>
	    <match lemma="destexane" tags="n.*"/>
